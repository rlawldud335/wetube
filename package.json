{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning YouTube",
  "main": "index.js",
  "scripts": {
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "dev:server": "nodemon --exec babel-node src/init.js --ignore 'scss' --ignore 'static' ",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "build": "npm run build:server && npm run build:assets && cp -R src/static build && cp -R src/views build",
    "prebuild": "rm -rf build",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "author": "KimJiyeong",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.13.0",
    "@babel/polyfill": "^7.12.1",
    "autoprefixer": "^10.2.3",
    "aws-sdk": "^2.847.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.3.1",
    "mongoose": "^5.11.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^5.0.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "postcss-loader": "^4.2.0",
    "pug": "^3.0.0",
    "sass-loader": "^10.1.1",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "mini-css-extract-plugin": "^1.3.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "browserslist": [
    "defaults"
  ]
}
